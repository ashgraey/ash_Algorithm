for _ in range(3) :
    game_list = list(map(int, input().split()))
    # 0이 하나 도
    # 0이 둘 개
    # 0이 셋 걸
    # 0이 넷 윷
    # 0이 제로 모
    cnt = 0 
    for i in game_list :
        if i == 0 :
            cnt += 1

    if cnt == 1 :
        print("A")
    elif cnt == 2 :
        print("B")
    elif cnt == 3 :
        print("C")
    elif cnt == 4 :
        print("D")
    else :
        print("E")


