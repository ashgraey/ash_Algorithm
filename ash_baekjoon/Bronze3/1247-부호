import sys

for i in range(3) : 
    N = int(sys.stdin.readline())
    
    dec_tot = 0
    for j in range(N) :
        dec = int(sys.stdin.readline())
        dec_tot += dec

    if dec_tot == 0 : 
        print("0")
    elif dec_tot > 0 :
        print("+")
    else :
        print("-")
    # print(dec_tot)